#notice
  = notice

%p
  %b
    Name:
  = @agency.name

%p
  %b
    Address:</b>
  = @agency.street1
  -if !@agency.street2.nil? 
    =@agency.street2
  = @agency.city + "," + @agency.province
  = @agency.postal_code
  

%p
  %b
    Phone:</b>
  = @agency.phone_number
    
- @agency.day_homes.each do |dayhome|
  %p
    = dayhome.name    

= link_to 'Edit', edit_admin_agency_path(@admin_agency) 
|
= link_to 'Back', admin_agencies_path 
