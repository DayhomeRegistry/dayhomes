.row.registrationStyle
  .span5.col-lg-5.col-md-5.col-sm-5.well.form-horizontal
    %h2
      Logged in as
      = current_user.full_name

    %p
      %strong Email:
      = current_user.email
      %br
      = link_to 'Edit Account', edit_user_path(current_user)
      |
      = link_to 'Logout', logout_path